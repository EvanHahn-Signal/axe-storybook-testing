{
  "name": "@chanzuckerberg/axe-storybook",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "bin": {
    "axe-storybook": "./bin/axe-storybook.js"
  },
  "main": "build/index.js",
  "files": [
    "bin",
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "babel --extensions '.js,.ts' src -d build",
    "clean": "rimraf ./build",
    "demo": "yarn build && yarn --cwd demo storybook:axe",
    "demo:install": "yarn --cwd demo install",
    "demo:link": "yarn link && yarn --cwd demo link @chanzuckerberg/axe-storybook",
    "demo:storybook": "yarn --cwd demo storybook",
    "demo:unlink": "yarn --cwd demo unlink @chanzuckerberg/axe-storybook && yarn unlink",
    "lint": "eslint --ext=js,jsx,ts,tsx .",
    "prebuild": "yarn clean",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "axe-core": "3.5.5",
    "axe-puppeteer": "^1.1.0",
    "debug": "^4.1.1",
    "puppeteer": "^1.4.0",
    "ts-dedent": "^1.1.1",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.13",
    "@types/mock-fs": "^4.10.0",
    "@types/node": "^14.10.0",
    "@types/puppeteer": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.8.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-react": "^7.20.6",
    "jest": "^26.4.2",
    "mock-fs": "^4.13.0",
    "rimraf": "^3.0.0",
    "typescript": "^4.0.2"
  }
}
