{
  "name": "@chanzuckerberg/axe-storybook",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "bin": {
    "axe-storybook": "./bin/axe-storybook.js"
  },
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "lint": "eslint --ext .js src/ tests/",
    "build": "babel src -d lib --ignore 'src/__tests__'",
    "prebuild": "rimraf ./lib",
    "prepublishOnly": "yarn build",
    "test:unit": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/eslint-parser": "^7.11.4",
    "@babel/eslint-plugin": "^7.11.3",
    "@babel/preset-env": "^7.11.0",
    "babel-jest": "^26.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-react": "^7.20.5",
    "jest": "^26.4.2",
    "mock-fs": "^4.12.0",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "puppeteer": "^1.4.0",
    "yargs": "^7.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "globals": {
      "window": true
    }
  },
  "jest": {
    "testMatch": [
      "**/tests/**-tests.js"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
